#include "stdio.h"
#include "stdlib.h"
struct NgaySinh{
	int ngay;
	int thang;
	int nam;
};
int main(){
	FILE *f;
	f = fopen("study", "rb");
	if(f == NULL){
		printf("\n Loi mo file!");
		exit(1);
	}
	NgaySinh n1;
	fread(&n1, sizeof(struct NgaySinh), 1, f);

	printf("\n Ngay sinh: %d/%d/%d", n1.ngay, n1.thang, n1.nam);

	// Dong file
	fclose(f);
}
